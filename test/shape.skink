const int PI = 3.14159265;
class Box {
    double l;
    double w;
    double h;

    void constructor(Box this, double l, double w, double h) {
        this.l = l;
        this.w = w;
        this.h = h;
    }

    double getSurfaceArea(Box this) {
        return 2 * (w * l + h * l  + h * w);
    }

    double getVolume(Box this) {
        return l * w * h;
    }
}

class Sphere {
    double radius;

    void constructor(Sphere this, double radius) {
        this.radius = radius;
    }

    double getDiameter() {
        return 2 * radius;
    }

    double getSurfaceArea() {
        return 4 * PI * pow(this.radius, 2);
    }

    double getVolume() {
        return (4 / 3) * PI * pow(this.radius, 3);
    }
}

Box myBox = new Box(1, 3, 3);
Sphere mySphere = new Sphere(1);

println(myBox.getVolume() == 9); 
println(mySphere.getVolume() == 4 * PI);