class Point {
    var constructor = (this, x, y) -> {
        this.x = x
        this.y = y
    }

    var add = (this, other) -> {
        return new Point(this.x + other.x, this.y + other.y)
    }
}

////////////////////////////////////////
var a = new Point(1, 1)
var b = new Point(1, 2)

Console.println(a + b)