    "api-site:build": "typedoc api.d.ts --out dev/docs --excludeExternals --excludePrivate",
    "api-site:upload": "./scripts/api-site-upload.sh",
    "build": "npm-run-all clean lint compile build:esm dts api-site:build bundle",
